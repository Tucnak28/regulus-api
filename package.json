{
  "name": "regulus-api",
  "version": "1.1.2",
  "description": "Regulus Rest API",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run build && npm run format && node ./dist/index.js",
    "log": "node ./scripts/HeatPumpLogger.js",
    "dev": "npm run lint && tsc-watch --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "test:unit": "jest --config=jest.config.js",
    "test:integration": "jest --config=jest.integration.config.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haluska77/regulus-api.git"
  },
  "keywords": [],
  "author": "Stanislav Haluska",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haluska77/regulus-api/issues"
  },
  "homepage": "https://github.com/haluska77/regulus-api#readme",
  "dependencies": {
    "axios": "1.1.3",
    "dotenv-extended": "^2.9.0",
    "express": "^4.21.1",
    "iconv-lite": "^0.6.3",
    "path": "^0.12.7",
    "swagger-ui-express": "^5.0.1",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8",
    "zod-openapi": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.13.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.16.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.16.0"
  }
}
